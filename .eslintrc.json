{
  "parserOptions": {
    "parser": "typescript-eslint-parser"
  },
  "extends": [
    "airbnb",
    "typescript"
  ],
  "plugins": [
    "react-hooks"    
  ],
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx", ".tsx"]  }]
  }
}